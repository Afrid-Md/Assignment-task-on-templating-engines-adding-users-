<%- include('./includes/head.ejs')%>
    <link rel="stylesheet" href="/css/users.css" />
</head>
<body>
    <%- include("./includes/navigation.ejs")%>
      <main class="main-body__users">
        <% if(users.length <= 0){ %>
          <h1 class="users-notFound">No Users Found...</h1>
        <%} else{%>
          <ul class="users-item__list">
            <% for(let user of users){ %>
            <li class="users-item"><%= user%></li>
            <%}%>
          </ul>
        <%}%>
      </main>
<%- include("./includes/end.ejs")%>
